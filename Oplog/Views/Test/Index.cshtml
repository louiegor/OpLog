                    <section class="st-panel" id="st-panel-1">
                        <div class="st-deco" data-icon="L"></div>
                       
                        <div class="table">
                            <p style="float:left"><strong>Page:</strong> {{tableParams.page()}} of {{tableParams.count()}}</p>
                            <span style="float:right"><strong style="text-align: right">Filter:</strong> <input class="form-control" type="text" ng-model="filter.$" /></span>
                        
                            <table ng-table="tableParams" class="table">
                                <tr ng-repeat="char in $data">
                                    <td data-title="'Name'">
                                        <nobr><img width="24" height="24" ng-src="{{char.iconPath}}"/> {{char.name}} </nobr>
                                    </td>
                                    <td data-title="'Organization'">
                                        {{char.orgName}}
                                    </td>
                                    <td data-title="'Type'">
                                        {{char.orgType}}
                                    </td>
                                    <td>
                                        {{char.description}}
                                    </td>
                                    <td>
                                        {{char.stats}}
                                    </td>
                                </tr>
                            </table>
                        </div>

